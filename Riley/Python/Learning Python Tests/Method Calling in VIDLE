#Calling Classes in VIDLE

from random import *

class Game(object):
    def rollDice(self,number):
        sum = 0
        for x in range(number):
            roll =  + randint(1,6)
            sum = sum + roll
            print "Dice", x+1, "rolled", roll
        print sum
    
# 1. g=Game()
# 2. g.rollDice(#)
